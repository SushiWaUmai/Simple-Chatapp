#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/code/Coding/ts/learn/Simple-Chatapp/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/bin/node_modules:/mnt/code/Coding/ts/learn/Simple-Chatapp/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/node_modules:/mnt/code/Coding/ts/learn/Simple-Chatapp/node_modules/.pnpm/typescript@4.5.5/node_modules:/mnt/code/Coding/ts/learn/Simple-Chatapp/node_modules/.pnpm/node_modules:/mnt/code/Coding/ts/learn/Simple-Chatapp/node_modules:/mnt/code/Coding/ts/learn/node_modules:/mnt/code/Coding/ts/node_modules:/mnt/code/Coding/node_modules:/mnt/code/node_modules:/mnt/node_modules:/node_modules:/mnt/code/Coding/ts/learn/Simple-Chatapp/packages/shared/node_modules:/mnt/code/Coding/ts/learn/Simple-Chatapp/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/code/Coding/ts/learn/Simple-Chatapp/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/bin/node_modules:/mnt/code/Coding/ts/learn/Simple-Chatapp/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/node_modules:/mnt/code/Coding/ts/learn/Simple-Chatapp/node_modules/.pnpm/typescript@4.5.5/node_modules:/mnt/code/Coding/ts/learn/Simple-Chatapp/node_modules/.pnpm/node_modules:/mnt/code/Coding/ts/learn/Simple-Chatapp/node_modules:/mnt/code/Coding/ts/learn/node_modules:/mnt/code/Coding/ts/node_modules:/mnt/code/Coding/node_modules:/mnt/code/node_modules:/mnt/node_modules:/node_modules:/mnt/code/Coding/ts/learn/Simple-Chatapp/packages/shared/node_modules:/mnt/code/Coding/ts/learn/Simple-Chatapp/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/bin/tsc" "$@"
fi
